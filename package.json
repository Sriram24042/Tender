{
  "name": "chainfly",
  "version": "1.0.0",
  "description": "Chainfly - Tender Management System",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd chainfly-backend && uvicorn app.main:app --reload --port 8000",
    "dev:frontend": "cd frontend && npm run dev",
    "install:all": "npm install && cd frontend && npm install && cd ../chainfly-backend && pip install -r requirements.txt",
    "build": "cd frontend && npm run build",
    "preview": "cd frontend && npm run preview"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": ["tender", "management", "react", "fastapi", "typescript"],
  "author": "Chainfly Team",
  "license": "MIT"
} 